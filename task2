#include<iostream>
using namespace std;

class Rectangle
{
    int length ,breadth;
    public:
    void Area(int length,int breadth)
    {
        length = length;
        breadth = breadth;
        cout<<"Area is "<<length*breadth;
    
    }
    void perimeter(int length,int breadth)
    {
        length = 50;
        breadth = 60;
        cout<<endl<<"length = "<<length;
        cout<<endl<<"breadth ="<<breadth;
        cout<<endl<<"Perimeter is "<<2*(length+breadth);
    }
    
};

int main()
{
    Rectangle r1;
    r1.Area(10,20);
    r1.perimeter(10,20);
    
}
